{
  "name": "@dotgems/react",
  "version": "0.1.5",
  "description": ".gems NFT UI library",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/react.esm.js",
  "unpkg": "dist/react.umd.production.min.js",
  "repository": {
    "type": "git",
    "url": "git+http://dotGems.github.io/gems-ui-library.git"
  },
  "authors": [
    "Dominic Fournier <dominic@eosnation.io>",
    "Jay Bhavsar <jay@eosnation.io>",
    "Denis Carriere <denis@eosnation.io>"
  ],
  "bugs": {
    "url": "http://dotGems.github.io/gems-ui-library/issues"
  },
  "homepage": "http://dotGems.github.io/gems-ui-library",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "prepare": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.0",
    "@mui/styles": "^5.2.0",
    "anchor-link": "^3.3.5",
    "anchor-link-browser-transport": "^3.2.5",
    "axios": "^0.26.0",
    "eosjs": "^22.1.0",
    "isomorphic-fetch": "^3.0.0",
    "moment": "^2.29.1",
    "scatter-ts": "^0.1.9",
    "swiper": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addons": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "auto": "^10.32.6",
    "babel-loader": "^8.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/react.umd.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react.esm.js",
      "limit": "10 KB"
    }
  ]
}
